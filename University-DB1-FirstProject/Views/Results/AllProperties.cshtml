@using University_DB1_FirstProject.Models
@model List<University_DB1_FirstProject.Models.PropertyDisplayModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Properties</title>
</head>
<body>
<h1>List of active properties</h1>
<div>
<ul>
    @foreach (PropertyDisplayModel p in Model)
    {
        <li> Property number: @p.PropertyNumber Value: @p.Value Address: @p.Address </li>
    }
</ul>
</div>
</body>
</html>